@article{alexanderImpactCarPollution2022,
  title = {The {{Impact}} of {{Car Pollution}} on {{Infant}} and {{Child Health}}: {{Evidence}} from {{Emissions Cheating}}},
  shorttitle = {The {{Impact}} of {{Car Pollution}} on {{Infant}} and {{Child Health}}},
  author = {Alexander, Diane and Schwandt, Hannes},
  year = {2022},
  month = feb,
  journal = {The Review of Economic Studies},
  pages = {rdac007},
  issn = {0034-6527},
  doi = {10.1093/restud/rdac007},
  urldate = {2022-06-13},
  abstract = {In 2008, Volkswagen introduced a new generation of ``Clean Diesel'' cars and heavily marketed them to environmentally conscious US consumers. Unknown to the public, these cars were anything but clean, emitting pollutants up to 150 times the level of comparable gas-fuelled cars. We study the rollout of these emissions-cheating diesel cars across the United States from 2008 to 2015 as a natural experiment to examine the impact of moderate levels of car pollution on infant and child health in the general population. Using the universe of vehicle registrations, we find that an additional cheating diesel car per 1,000 cars increases \${\textbackslash}mathrm\{PM\_\{2.5\}\}\$, \${\textbackslash}mathrm\{PM\_\{10\}\}\$, and ozone by 2, 2.2, and 1.3\${\textbackslash}\%\$, respectively, while the low birth weight rate and infant mortality rate increase by 1.9 and 1.7\${\textbackslash}\%\$, respectively. Similar impacts are found for acute asthma attacks in children. These health impacts occur at all pollution levels and across the socioeconomic spectrum.},
  file = {/Users/esaulnier/Zotero/storage/D3KEDDI2/Alexander and Schwandt - 2022 - The Impact of Car Pollution on Infant and Child He.pdf;/Users/esaulnier/Zotero/storage/RVJPP39U/6529413.html}
}

@article{almondCostsLowBirth2005,
