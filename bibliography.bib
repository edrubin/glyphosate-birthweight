@article{perryHistoricalEvidenceGlyphosate2019,
  title = {Historical Evidence of Glyphosate Exposure from a {{US}} Agricultural Cohort},
  author = {Perry, Melissa J. and Mandrioli, Daniele and Belpoggi, Fiorella and Manservisi, Fabiana and Panzacchi, Simona and Irwin, Courtney},
