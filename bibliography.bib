@article{banzhafEnvironmentalJusticeEconomics2019,
  title = {Environmental {{Justice}}: {{The Economics}} of {{Race}}, {{Place}}, and {{Pollution}}},
  shorttitle = {Environmental {{Justice}}},
  author = {Banzhaf, Spencer and Ma, Lala and Timmins, Christopher},
