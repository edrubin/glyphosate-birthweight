@article{sagivPrenatalExposureOrganophosphate2019,
  title = {Prenatal Exposure to Organophosphate Pesticides and Functional Neuroimaging in Adolescents Living in Proximity to Pesticide Application},
  author = {Sagiv, Sharon K. and Bruno, Jennifer L. and Baker, Joseph M. and Palzes, Vanessa and Kogut, Katherine and Rauch, Stephen and Gunier, Robert and Mora, Ana M. and Reiss, Allan L. and Eskenazi, Brenda},
  year = {2019},
